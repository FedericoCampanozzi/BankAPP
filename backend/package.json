{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev":"nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node ./src/index.ts",
		"dev-mysql": "npm run dev -- mysql",
		"dev-postgres": "npm run dev -- postgres",
		"dev-sqlite": "npm run dev -- _sqlite",
		"typeorm:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed ",
		"typeorm:seed-mysql":"npm run typeorm:seed -- -n 'src/db/ormconfig/ormconfig.mysql.json'",
		"typeorm:seed-postgres":"npm run typeorm:seed -- -n 'src/db/ormconfig/ormconfig.postgres.json'",
		"typeorm:seed-sqlite":"npm run typeorm:seed -- -n 'src/db/ormconfig/ormconfig.sqlite.json'",
		"typeorm:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/db/migrations",
		"typeorm:generate-mysql" : "npm run typeorm:generate -- -f src/db/ormconfig/ormconfig.mysql.json",
		"typeorm:generate-postgres" : "npm run typeorm:generate -- -f src/db/ormconfig/ormconfig.postgres.json",
		"typeorm:generate-sqlite" : "npm run typeorm:generate -- -f src/db/ormconfig/ormconfig.sqlite.json",
		"d-ms" : "npm run dev-mysql --",
		"d-p" : "npm run dev-postgres --",
		"d-s" : "npm run dev-sqlite --",
		"ts-ms" : "npm run typeorm:seed-mysql --",
		"ts-p" : "npm run typeorm:seed-postgres --",
		"ts-s" : "npm run typeorm:seed-sqlite --",
		"tg-ms" : "npm run typeorm:generate-mysql --",
		"tg-p" : "npm run typeorm:generate-postgres --",
		"tg-s" : "npm run typeorm:generate-sqlite --"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/express": "^4.17.12",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"faker": "^6.6.6",
		"mysql": "^2.18.1",
		"pg": "^8.6.0",
		"sqlite3": "^5.1.6",
		"ts-node": "^10.0.0",
		"typeorm": "^0.2.34",
		"typeorm-seeding": "^1.6.1",
		"typescript": "^4.3.5"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/faker": "^6.6.9",
		"ts-node-dev": "^1.1.8"
	}
}
