{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev":"nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node ./src/index.ts",
		"dev-mysql": "npm run dev -- mysql",
		"dev-sqlite": "npm run dev _sqlite",
		"dev-postgres": "npm run dev postgres",
		"typeorm:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed ",
		"s-mysql":"npm run typeorm:seed -- -n 'src/db/ormconfig/ormconfig.mysql.json'",
		"s-postgres":"npm run typeorm:seed -- -n 'src/db/ormconfig/ormconfig.postgres.json'",
		"s-sqlite":"npm run typeorm:seed -- -n 'src/db/ormconfig/ormconfig.sqlite.json'"
	},
