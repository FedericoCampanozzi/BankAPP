		"@types/express": "^4.17.12",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"faker": "^6.6.6",
		"mysql": "^2.18.1",
		"pg": "^8.6.0",
		"sqlite3": "^5.1.6",
		"ts-node": "^10.0.0",
		"typeorm": "^0.2.34",
		"typeorm-seeding": "^1.6.1",
		"typescript": "^4.3.5"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/faker": "^6.6.9",
