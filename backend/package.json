{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev-mysql": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node ./src/index.ts mysql",
		"dev-sqlite": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node ./src/index.ts _sqlite'",
		"dev-postgres": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node ./src/index.ts postgres'",
		"db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
		"seed:run": "ts-node typeorm migration:run -c seed"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/express": "^4.17.12",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"mysql": "^2.18.1",
		"pg": "^8.6.0",
		"sqlite3": "^5.1.6",
		"ts-node": "^10.0.0",
		"typeorm": "^0.2.34",
		"typeorm-seeding": "^1.6.1",
		"typescript": "^4.3.5"
	},
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"ts-node-dev": "^1.1.8"
	}
}
